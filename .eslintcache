jectList=props.cources.map(x=><CourseElement isAuthor={x.teacher_id===props.user_id} handleClick={handleClick} id={x._id} author={x.author}title={x.name}/>)\r\n    return(\r\n        <div className={s.wrapper}>\r\n            <Header title={\"Все курсы\"}/>\r\n            <div className={s.container}>\r\n                {AllObjectList}\r\n            </div>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nconst mapStateToProps = (state) => ({\r\n    cources: state.TeacherReducer.cources,\r\n    user_id: state.AuthReducer.user_id,\r\n  })\r\nexport default  connect(mapStateToProps, { Get_Data_Async })(MainContainer);","C:\\Users\\Dina\\Desktop\\учеба\\diploma\\src\\components\\TeacherPage\\molecule\\CourseElement\\CourseElement.jsx",[],"C:\\Users\\Dina\\Desktop\\учеба\\diploma\\src\\components\\router\\Teacher.jsx",[],"C:\\Users\\Dina\\Desktop\\учеба\\diploma\\src\\components\\TeacherPage\\organism\\CoursePage\\CoursePage.jsx",["151","152","153","154","155","156","157"],"import React, { useEffect, useState } from 'react';\r\nimport { Redirect, useLocation } from 'react-router-dom';\r\nimport { Get_Data_Async, UpdateCourse, Delete_Course_Async } from '../../../../redux/reducers/TeacherReducer'\r\nimport { connect } from 'react-redux';\r\nimport s from './CoursePage.module.scss';\r\nimport Header from '../../../utils/Header/Header';\r\nimport { useHistory } from 'react-router-dom';\r\nimport AddSection from '../../atom/AddSection/AddSection';\r\nimport deleteicon from '../../../sources/delete.png'\r\nimport Button from '../../../utils/Button/Button';\r\n\r\nconst CoursePage = (props) => {\r\n    const [showAdd, setshowAdd] = useState(false);\r\n    const history = useHistory();\r\n    useEffect(() => {\r\n        props.Get_Data_Async()\r\n    }, [props.cources])\r\n    const location = useLocation()\r\n    const handleClick = (id) => {\r\n        history.push(`/teacherPage/my_cources/${location.pathname.split('/').pop()}/${id}`);\r\n    }\r\n    const [course, setCourse] = u